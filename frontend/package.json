{
  "name": "falco",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/react": "^8.0.1",
    "@types/react-modal": "^3.8.2",
    "@types/react-redux-toastr": "^7.4.1",
    "@types/react-select": "^2.0.13",
    "@types/recharts": "^1.1.16",
    "@types/redux-mock-store": "^1.0.1",
    "babel-polyfill": "6.26.0",
    "connected-react-router": "5.0.1",
    "core-js": "^3.3.3",
    "dayjs": "^1.8.8",
    "env-cmd": "8.0.2",
    "formik": "1.3.2",
    "history": "4.7.2",
    "jest-dom": "^3.4.0",
    "jwt-decode": "^2.2.0",
    "lodash": "4.17.15",
    "react": "^16.8.3",
    "react-app-rewire-styled-components": "3.0.2",
    "react-dom": "^16.8.3",
    "react-intl": "2.7.2",
    "react-modal": "^3.8.1",
    "react-redux": "5.1.0",
    "react-redux-toastr": "^7.5.1",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-scripts": "3.3.0",
    "react-select": "^3.0.4",
    "react-switch": "^5.0.1",
    "react-use": "^12.5.0",
    "recharts": "^1.5.0",
    "redux": "4.0.1",
    "redux-mock-store": "^1.5.3",
    "redux-persist": "5.10.0",
    "redux-saga": "0.16.2",
    "styled-components": "4.1.1",
    "superagent": "4.0.0",
    "typesafe-actions": "^3.0.0"
  },
  "scripts": {
    "start": "BROWSER=none react-app-rewired start",
    "build": "PUBLIC_URL=/static NODE_PATH=src/ NODE_ENV=production react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "analyze": "env-cmd .env.${REACT_APP_ENV} react-app-rewired build --analyze-bundle",
    "test:coverage": "npm run test -- --coverage",
    "generate": "plop --plopfile scripts/generators/index.js",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "lint:fix": "tslint -c tslint.json 'src/**/*.{ts,tsx}' --fix",
    "lint-style": "stylelint 'src/**/*.style.{ts,tsx}' 'src/**/style.{ts,tsx}'"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js",
      "!src/**/actions.js",
      "!src/**/index.js",
      "!src/**/serviceWorker.js",
      "!src/**/*.wrap.js",
      "!src/index.js",
      "!src/tempPolyfills.js",
      "!src/setupTests.js",
      "!src/redux/reducers.js",
      "!src/redux/sagas.js",
      "!src/redux/store.js"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 0,
        "branches": 0,
        "functions": 0,
        "lines": 0
      }
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ]
  },
  "bundlesize": [
    {
      "path": "./build/static/js/*.js",
      "maxSize": "200 kB"
    }
  ],
  "devDependencies": {
    "@types/cheerio": "^0.22.10",
    "@types/enzyme": "^3.1.15",
    "@types/history": "^4.7.2",
    "@types/jest": "^23.3.12",
    "@types/jwt-decode": "^2.2.1",
    "@types/lodash": "^4.14.119",
    "@types/node": "^10.12.18",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-intl": "^2.3.17",
    "@types/react-redux": "^6.0.12",
    "@types/react-router": "^4.4.3",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-test-renderer": "^16.0.3",
    "@types/styled-components": "^4.1.5",
    "@types/superagent": "^3.8.6",
    "@types/webpack-env": "^1.13.6",
    "babel-plugin-styled-components": "1.8.0",
    "cypress": "^3.1.4",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.7.0",
    "enzyme-to-json": "3.3.4",
    "eslint-config-prettier": "3.3.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jest": "22.0.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-mysticatea": "4.2.4",
    "eslint-plugin-prefer-object-spread": "1.2.1",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-react": "7.11.1",
    "plop": "2.4.0",
    "prettier": "1.15.1",
    "react-app-rewire-webpack-bundle-analyzer": "1.1.0",
    "react-app-rewired": "2.1.3",
    "react-test-renderer": "16.6.3",
    "redux-saga-test-plan": "3.7.0",
    "stylelint": "^10.1.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-declaration-strict-value": "^1.1.2",
    "stylelint-processor-styled-components": "^1.5.2",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-react": "^3.6.0",
    "typescript": "~3.4.0",
    "webpack-manifest-plugin": "^2.0.4"
  }
}
